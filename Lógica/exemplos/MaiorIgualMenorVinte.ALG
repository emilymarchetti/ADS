Algoritmo "MaiorIgualMenorVinte"

Var

   num: inteiro

Inicio

   escreva ("Insira um número: ")
   leia (num)
   se num >20 entao
   escreval ("")
      escreval ("O número é maior que 20")
   fimse
   se num=20 entao
      escreval ("O número é igual a 20")
      escreval ("")
   fimse
   se num<20 entao
      escreval ("O número é menor que 20")
      escreval ("")
   fimse

Fimalgoritmo